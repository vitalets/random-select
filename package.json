    "eslint": "^8.9.0",
    "eslint-plugin-mocha": "^10.0.3",
    "lint-staged": "^12.3.4",
    "mocha": "^9.2.0",
    "np": "^7.6.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
